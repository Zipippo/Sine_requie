{
    "id": "custom-system-builder",
    "title": "Custom System Builder",
    "description": "Build your own system with easy to use components",
    "version": "3.0.0",
    "compatibility": {
        "minimum": "11",
        "verified": "11"
    },
    "authors": [
        {
            "name": "Jean-Baptiste Louvet-Daniel",
            "email": "linkedfluuuush@gmail.com",
            "discord": "LinkedFluuuush"
        },
